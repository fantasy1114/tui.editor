{
    "extends": [
        "tui"
    ],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "classes": true,
            "spread": true,
            "defaultParams": true,
            "destructuring": true
        }
    },
    "env":{
        "es6": true,
        "amd": true,
        "jasmine": true,
        "jquery": true
    },
    "globals": {
        "tui": true,
        "loadFixtures": true,
        "ActiveXObject": true
    },
    "rules": {
        "linebreak-style": 0,
        "max-len": [2, {"code": 120, "tabWidth": 4, "ignoreStrings": true, "ignoreComments": true, "ignoreRegExpLiterals": true, "ignoreTemplateLiterals": true}]
    }
}
